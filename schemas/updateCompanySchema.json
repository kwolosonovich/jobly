{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "The root schema",
  "description": "The root schema comprises the entire JSON document.",
  "default": {},
  "examples": [
    {
      "handle": "newCompany",
      "name": "New",
      "num_employees": 10,
      "description": "desc",
      "logo_url": "logoUrl"
    }
  ],
  "properties": {
    "handle": {
      "$id": "#/properties/handle",
      "type": "string",
      "title": "The handle schema",
      "description": "Company handle name.",
      "default": "",
      "examples": ["RefineryNet"]
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The name schema",
      "description": "Company name.",
      "default": "",
      "examples": ["Refinery Network"]
    },
    "num_employees": {
      "$id": "#/properties/num_employees",
      "type": "integer",
      "title": "The num_employees schema",
      "description": "Number of employees",
      "default": 0,
      "examples": [10]
    },
    "description": {
      "$id": "#/properties/description",
      "type": "string",
      "title": "The description schema",
      "description": "Company description.",
      "default": "",
      "examples": ["Best tech company"]
    },
    "logo_url": {
      "$id": "#/properties/logo_url",
      "type": "string",
      "title": "The logo_url schema",
      "description": "Company logo URL.",
      "default": "",
      "examples": ["https://picsum.photos/200/300"]
    }
  }
}
